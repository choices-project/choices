        try {
            // Redirect to Google OAuth
            const { safeNavigate } = await Promise.resolve().then(()=>/*#__PURE__*/ _interop_require_wildcard(require("../../../lib/utils/ssr-safe")));
            safeNavigate('/auth/google');
