const devLog = (...args)=>{
    if (process.env.NODE_ENV === 'development') {
        // eslint-disable-next-line no-console
        console.debug('[DEV]', ...args);
    }
};
const logInfo = (...args)=>{
    // eslint-disable-next-line no-console
    console.info(...args);
};
const logWarn = (...args)=>{
    // eslint-disable-next-line no-console
    console.warn(...args);
};
const logError = (...args)=>{
    // eslint-disable-next-line no-console
    console.error(...args);
};
const logger = {
    debug: devLog,
    info: logInfo,
    warn: logWarn,
    error: logError,
    performance: logInfo
};

