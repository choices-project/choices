        (0, _globals.expect)(mockFlag.flag_type).toBe('inappropriate');
        (0, _globals.expect)(mockFlag.status).toBe('pending');
    });
    (0, _globals.it)('should calculate auto-moderation scores correctly', ()=>{
        // Test the auto-moderation scoring logic
        const calculateScore = (hashtagName)=>{
            let score = 0;
            const name = hashtagName.toLowerCase();
            // Spam keywords
            const spamKeywords = [
                'buy',
                'sell',
                'promo',
                'discount',
                'offer',
                'deal'
            ];
            const spamMatches = spamKeywords.filter((keyword)=>name.includes(keyword));
            score += spamMatches.length * 0.3;
            // Inappropriate keywords
            const inappropriateKeywords = [
                'hate',
                'violence',
                'explicit',
                'nsfw'
            ];
            const inappropriateMatches = inappropriateKeywords.filter((keyword)=>name.includes(keyword));
            score += inappropriateMatches.length * 0.4;
            // Misleading patterns
            const misleadingPatterns = [
                /fake/i,
                /scam/i,
                /fraud/i,
                /lie/i,
                /false/i
            ];
            const misleadingMatches = misleadingPatterns.filter((pattern)=>pattern.test(name));
            score += misleadingMatches.length * 0.2;
            // Special characters
            const specialCharCount = (name.match(/[^a-z0-9]/g) || []).length;
            if (specialCharCount > name.length * 0.3) {
                score += 0.2;
            }
            // Repetitive characters
            const repetitivePattern = /(.)\1{2,}/;
            if (repetitivePattern.test(name)) {
                score += 0.3;
            }
            return Math.min(score, 1.0);
        };
        // Test clean hashtag
        (0, _globals.expect)(calculateScore('climate-change')).toBeLessThan(0.5);
        // Test spam hashtag
        (0, _globals.expect)(calculateScore('buy-now-discount-promo')).toBeGreaterThan(0.7);
        // Test inappropriate hashtag
        (0, _globals.expect)(calculateScore('hate-speech')).toBeGreaterThan(0.3);
        // Test misleading hashtag
        (0, _globals.expect)(calculateScore('fake-news')).toBeGreaterThanOrEqual(0.2);
        // Test repetitive hashtag
        (0, _globals.expect)(calculateScore('aaaaa')).toBeGreaterThanOrEqual(0.3);
    });
    (0, _globals.it)('should validate moderation workflow states', ()=>{
        // Test that moderation states are properly defined
        const validStatuses = [
            'approved',
            'pending',
            'rejected',
            'flagged'
        ];
