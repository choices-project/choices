function formatUsageCount(count) {
    if (count < 1000) {
        return count.toString();
    }
    if (count < 1000000) {
        return `${(count / 1000).toFixed(1)}K`;
    }
    if (count < 1000000000) {
        return `${(count / 1000000).toFixed(1)}M`;
    }
    return `${(count / 1000000000).toFixed(1)}B`;
}
function formatEngagementRate(rate) {
    return `${(rate * 100).toFixed(1)}%`;
}
function formatGrowthRate(rate) {
    const sign = rate >= 0 ? '+' : '';
    return `${sign}${(rate * 100).toFixed(1)}%`;
}
function formatTrendingScore(score) {
    if (score < 1) {
        return score.toFixed(2);
    }
    if (score < 10) {
        return score.toFixed(1);
    }
    return Math.round(score).toString();
}
function formatCurrency(amount, currency = 'USD') {
    return new Intl.NumberFormat('en-US', {
        style: 'currency',
        currency
    }).format(amount);
}
function formatNumber(num) {
    return new Intl.NumberFormat('en-US').format(num);
}
function formatDate(date) {
    const d = typeof date === 'string' ? new Date(date) : date;
    return new Intl.DateTimeFormat('en-US', {
        year: 'numeric',
        month: 'short',
        day: 'numeric'
    }).format(d);
}
function formatDateTime(date) {
    const d = typeof date === 'string' ? new Date(date) : date;
    return new Intl.DateTimeFormat('en-US', {
        year: 'numeric',
        month: 'short',
        day: 'numeric',
        hour: 'numeric',
        minute: '2-digit'
    }).format(d);
}
function formatRelativeTime(date) {
    const d = typeof date === 'string' ? new Date(date) : date;
    const now = new Date();
    const diffInSeconds = Math.floor((now.getTime() - d.getTime()) / 1000);
    if (diffInSeconds < 60) {
        return 'just now';
    }
    if (diffInSeconds < 3600) {
        const minutes = Math.floor(diffInSeconds / 60);
        return `${minutes} minute${minutes !== 1 ? 's' : ''} ago`;
    }
    if (diffInSeconds < 86400) {
        const hours = Math.floor(diffInSeconds / 3600);
        return `${hours} hour${hours !== 1 ? 's' : ''} ago`;
    }
    if (diffInSeconds < 2592000) {
        const days = Math.floor(diffInSeconds / 86400);
        return `${days} day${days !== 1 ? 's' : ''} ago`;
    }
    return formatDate(d);
}
function formatDisplayName(name) {
    return name.split(' ').map((word)=>word.charAt(0).toUpperCase() + word.slice(1).toLowerCase()).join(' ');
}
function formatHashtagName(name) {
    return name.toLowerCase().replace(/[^a-z0-9_-]/g, '_').replace(/_+/g, '_').replace(/^_|_$/g, '');
}
function truncateText(text, maxLength) {
    if (text.length <= maxLength) {
        return text;
    }
    return `${text.slice(0, maxLength - 3)}...`;
}
function formatFileSize(bytes) {
    if (bytes === 0) return '0 Bytes';
    const k = 1024;
    const sizes = [
        'Bytes',
        'KB',
        'MB',
        'GB',
        'TB'
    ];
    const i = Math.floor(Math.log(bytes) / Math.log(k));
    return `${parseFloat((bytes / Math.pow(k, i)).toFixed(2))} ${sizes[i]}`;
}
function formatPerformanceMetric(value, unit) {
    switch(unit){
        case 'ms':
            return `${value.toFixed(1)}ms`;
        case 'bytes':
            return formatFileSize(value);
        case 'count':
            return formatUsageCount(value);
        case 'score':
            return formatTrendingScore(value);
        default:
            return value.toString();
    }
}
function formatPercentage(value, decimals = 1) {
    return `${(value * 100).toFixed(decimals)}%`;
}
function isValidNumber(value) {
    return typeof value === 'number' && !isNaN(value) && isFinite(value);
}
function isValidDate(value) {
    return value instanceof Date && !isNaN(value.getTime());
}
function isValidEmail(email) {
    const emailRegex = /^[^\s@]+@[^\s@]+\.[^\s@]+$/;
    return emailRegex.test(email);
}
function isValidUrl(url) {
    try {
        new URL(url);
        return true;
    } catch  {
        return false;
    }
}

