                                                    activity.description && /*#__PURE__*/ (0, _jsxruntime.jsx)("p", {
                                                        className: "text-xs text-gray-500 mt-1",
                                                        children: activity.description
                                                    }),
                                                    /*#__PURE__*/ (0, _jsxruntime.jsxs)("p", {
                                                        className: "text-xs text-gray-400 mt-1",
                                                        children: [
                                                            /*#__PURE__*/ (0, _jsxruntime.jsx)(_outline.ClockIcon, {
                                                                className: "w-3 h-3 inline mr-1"
                                                            }),
                                                            new Date(activity.date).toLocaleDateString(),
                                                            activity.source && /*#__PURE__*/ (0, _jsxruntime.jsxs)("span", {
                                                                className: "ml-2 text-gray-500",
                                                                children: [
                                                                    "â€¢ ",
                                                                    activity.source
                                                                ]
                                                            })
                                                        ]
                                                    })
                                                ]
                                            })
                                        ]
                                    }, index))
                            })
                        ]
                    })
                ]
            }),
            /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                className: "p-4 border-t border-gray-200",
                children: /*#__PURE__*/ (0, _jsxruntime.jsxs)("button", {
                    onClick: ()=>setIsExpanded(!isExpanded),
                    className: "w-full flex items-center justify-center space-x-2 text-gray-600 hover:text-gray-900 transition-colors",
                    "aria-expanded": isExpanded,
                    "aria-label": isExpanded ? 'Show less information' : 'Show more information',
                    children: [
                        /*#__PURE__*/ (0, _jsxruntime.jsx)("span", {
                            className: "text-sm font-medium",
                            children: isExpanded ? 'Show Less' : 'Show More'
                        }),
                        isExpanded ? /*#__PURE__*/ (0, _jsxruntime.jsx)(_outline.ChevronUpIcon, {
                            className: "w-4 h-4"
                        }) : /*#__PURE__*/ (0, _jsxruntime.jsx)(_outline.ChevronDownIcon, {
                            className: "w-4 h-4"
                        })
                    ]
                })
            }),
            showPhotoModal && primaryPhoto && /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                className: "fixed inset-0 bg-black bg-opacity-50 flex items-center justify-center z-50 p-4",
                onClick: handlePhotoModalClose,
                children: /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                    className: "relative max-w-4xl max-h-full",
                    children: [
                        /*#__PURE__*/ (0, _jsxruntime.jsx)("button", {
                            onClick: handlePhotoModalClose,
                            className: "absolute top-4 right-4 z-10 p-2 bg-white rounded-full shadow-lg hover:bg-gray-50 transition-colors",
                            "aria-label": "Close photo modal",
                            children: /*#__PURE__*/ (0, _jsxruntime.jsx)(_outline.XMarkIcon, {
                                className: "w-6 h-6 text-gray-600"
                            })
                        }),
                        /*#__PURE__*/ (0, _jsxruntime.jsx)(_image.default, {
                            src: primaryPhoto.url,
                            alt: `${representative.name} official photo`,
                            width: 800,
                            height: 600,
                            className: "rounded-lg",
                            sizes: "(max-width: 768px) 100vw, (max-width: 1200px) 80vw, 60vw"
                        })
                    ]
                })
            })
        ]
    });
}

