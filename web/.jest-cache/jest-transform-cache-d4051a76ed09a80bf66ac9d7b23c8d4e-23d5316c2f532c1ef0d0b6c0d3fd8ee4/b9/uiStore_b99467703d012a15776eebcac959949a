const _zustand = require("zustand");
const _middleware = require("zustand/middleware");
const _immer = require("zustand/middleware/immer");
const _logger = require("../utils/logger");
const useUIStore = (0, _zustand.create)()((0, _middleware.devtools)((0, _middleware.persist)((0, _immer.immer)((set, get)=>({
        // Initial state
        activeTab: "feed",
        tabHistory: [
            "feed"
        ],
        // Modal state
        activeModal: null,
        modalData: null,
        modalStack: [],
        // Form state
        formData: {},
        formErrors: {},
        formTouched: {},
        // Loading states
        loadingStates: {},
        // Error states
        errors: {},
        // Toast states
        toasts: [],
        // Sidebar/Drawer states
        sidebarOpen: false,
        drawerOpen: false,
        // Search states
        searchQuery: "",
        searchResults: [],
        searchLoading: false,
        // Filter states
        filters: {},
        // Pagination states
        pagination: {},
        // Selection states
        selections: {},
        // View states
        viewMode: {},
        // Sort states
        sortBy: {},
        sortOrder: {},
        // Base store state
        isLoading: false,
        error: null,
        // Base store actions
        setLoading: (loading)=>set((state)=>{
                state.isLoading = loading;
            }),
        setError: (error)=>set((state)=>{
                state.error = error;
            }),
        clearError: ()=>set((state)=>{
                state.error = null;
            }),
        // Tab actions
        setActiveTab: (tab)=>set((state)=>{
                if (state.activeTab !== tab) {
                    state.tabHistory.push(state.activeTab);
                    state.activeTab = tab;
                    _logger.logger.info("Tab changed", {
                        from: state.tabHistory[state.tabHistory.length - 1],
                        to: tab
                    });
                }
            }),
        goBackTab: ()=>set((state)=>{
                if (state.tabHistory.length > 0) {
                    const previousTab = state.tabHistory.pop();
                    if (previousTab) {
                        state.activeTab = previousTab;
                        _logger.logger.info("Tab went back", {
                            to: previousTab
                        });
                    }
                }
            }),
        clearTabHistory: ()=>set((state)=>{
                state.tabHistory = [];
            }),
        // Modal actions
        openModal: (id, data)=>set((state)=>{
                state.activeModal = id;
                state.modalData = data;
                state.modalStack.push({
                    id,
                    data,
                    timestamp: Date.now()
                });
                _logger.logger.info("Modal opened", {
                    id,
                    data
                });
            }),
        closeModal: ()=>set((state)=>{
                if (state.modalStack.length > 0) {
                    state.modalStack.pop();
                    if (state.modalStack.length > 0) {
                        const previousModal = state.modalStack[state.modalStack.length - 1];
                        state.activeModal = previousModal.id;
                        state.modalData = previousModal.data;
                    } else {
                        state.activeModal = null;
                        state.modalData = null;
                    }
                } else {
                    state.activeModal = null;
                    state.modalData = null;
                }
                _logger.logger.info("Modal closed");
            }),
        closeAllModals: ()=>set((state)=>{
                state.activeModal = null;
                state.modalData = null;
                state.modalStack = [];
                _logger.logger.info("All modals closed");
            }),
        // Form actions
        setFormData: (key, value)=>set((state)=>{
                state.formData[key] = value;
                // Clear error when user starts typing
                if (state.formErrors[key]) {
                    state.formErrors[key] = null;
                }
            }),
        setFormError: (key, error)=>set((state)=>{
                state.formErrors[key] = error;
            }),
        setFormTouched: (key, touched)=>set((state)=>{
                state.formTouched[key] = touched;
            }),
        clearForm: (key)=>set((state)=>{
                if (key) {
                    delete state.formData[key];
                    delete state.formErrors[key];
                    delete state.formTouched[key];
                } else {
                    state.formData = {};
                    state.formErrors = {};
                    state.formTouched = {};
                }
            }),
        validateForm: (key)=>{
            const state = get();
            return !state.formErrors[key] && state.formTouched[key];
        },
        // Loading actions
        setLoading: (key, loading)=>set((state)=>{
                state.loadingStates[key] = loading;
            }),
        isLoading: (key)=>{
            const state = get();
            return state.loadingStates[key] || false;
        },
        clearLoading: (key)=>set((state)=>{
                delete state.loadingStates[key];
            }),
        clearAllLoading: ()=>set((state)=>{
                state.loadingStates = {};
            }),
        // Error actions
        setError: (key, error)=>set((state)=>{
                state.errors[key] = error;
            }),
        clearError: (key)=>set((state)=>{
                delete state.errors[key];
            }),
        clearAllErrors: ()=>set((state)=>{
                state.errors = {};
            }),
        hasError: (key)=>{
            const state = get();
            return !!state.errors[key];
        },
        // Toast actions
        addToast: (toast)=>set((state)=>{
                const id = `toast-${Date.now()}-${Math.random().toString(36).substr(2, 9)}`;
                state.toasts.push({
                    ...toast,
                    id,
                    timestamp: Date.now()
                });
                _logger.logger.info("Toast added", {
                    id,
                    type: toast.type,
                    message: toast.message
                });
            }),
        removeToast: (id)=>set((state)=>{
                state.toasts = state.toasts.filter((toast)=>toast.id !== id);
            }),
        clearToasts: ()=>set((state)=>{
                state.toasts = [];
            }),
        // Sidebar/Drawer actions
        toggleSidebar: ()=>set((state)=>{
                state.sidebarOpen = !state.sidebarOpen;
            }),
        toggleDrawer: ()=>set((state)=>{
                state.drawerOpen = !state.drawerOpen;
            }),
        setSidebarOpen: (open)=>set((state)=>{
                state.sidebarOpen = open;
            }),
        setDrawerOpen: (open)=>set((state)=>{
                state.drawerOpen = open;
            }),
        // Search actions
        setSearchQuery: (query)=>set((state)=>{
                state.searchQuery = query;
            }),
        setSearchResults: (results)=>set((state)=>{
                state.searchResults = results;
            }),
        setSearchLoading: (loading)=>set((state)=>{
                state.searchLoading = loading;
            }),
        clearSearch: ()=>set((state)=>{
                state.searchQuery = "";
                state.searchResults = [];
                state.searchLoading = false;
            }),
        // Filter actions
        setFilter: (key, value)=>set((state)=>{
                state.filters[key] = value;
            }),
        clearFilter: (key)=>set((state)=>{
                delete state.filters[key];
            }),
        clearAllFilters: ()=>set((state)=>{
                state.filters = {};
            }),
        // Pagination actions
        setPagination: (key, pagination)=>set((state)=>{
                state.pagination[key] = {
                    page: 1,
                    limit: 10,
                    total: 0,
                    hasMore: false,
                    ...state.pagination[key],
                    ...pagination
                };
            }),
        nextPage: (key)=>set((state)=>{
                if (state.pagination[key]) {
                    state.pagination[key].page += 1;
                }
            }),
        prevPage: (key)=>set((state)=>{
                if (state.pagination[key] && state.pagination[key].page > 1) {
                    state.pagination[key].page -= 1;
                }
            }),
        resetPagination: (key)=>set((state)=>{
                if (state.pagination[key]) {
                    state.pagination[key].page = 1;
                }
            }),
        // Selection actions
        selectItem: (key, itemId)=>set((state)=>{
                if (!state.selections[key]) {
                    state.selections[key] = new Set();
                }
                state.selections[key].add(itemId);
            }),
        deselectItem: (key, itemId)=>set((state)=>{
                if (state.selections[key]) {
                    state.selections[key].delete(itemId);
                }
            }),
        toggleSelection: (key, itemId)=>set((state)=>{
                if (!state.selections[key]) {
                    state.selections[key] = new Set();
                }
                if (state.selections[key].has(itemId)) {
                    state.selections[key].delete(itemId);
                } else {
                    state.selections[key].add(itemId);
                }
            }),
        clearSelection: (key)=>set((state)=>{
                if (state.selections[key]) {
                    state.selections[key].clear();
                }
            }),
        clearAllSelections: ()=>set((state)=>{
                state.selections = {};
            }),
        isSelected: (key, itemId)=>{
            const state = get();
            return state.selections[key]?.has(itemId) || false;
        },
        // View actions
        setViewMode: (key, mode)=>set((state)=>{
                state.viewMode[key] = mode;
            }),
        // Sort actions
        setSort: (key, by, order = "asc")=>set((state)=>{
                state.sortBy[key] = by;
                state.sortOrder[key] = order;
            }),
        toggleSort: (key, by)=>set((state)=>{
                if (state.sortBy[key] === by) {
                    state.sortOrder[key] = state.sortOrder[key] === "asc" ? "desc" : "asc";
                } else {
                    state.sortBy[key] = by;
                    state.sortOrder[key] = "asc";
                }
            }),
        // Utility actions
        resetUI: ()=>set((state)=>{
                state.activeTab = "feed";
                state.tabHistory = [
                    "feed"
                ];
                state.activeModal = null;
                state.modalData = null;
                state.modalStack = [];
                state.formData = {};
                state.formErrors = {};
                state.formTouched = {};
                state.loadingStates = {};
                state.errors = {};
                state.toasts = [];
                state.sidebarOpen = false;
                state.drawerOpen = false;
                state.searchQuery = "";
                state.searchResults = [];
                state.searchLoading = false;
                state.filters = {};
                state.pagination = {};
                state.selections = {};
                state.viewMode = {};
                state.sortBy = {};
                state.sortOrder = {};
                state.isLoading = false;
                state.error = null;
            }),
        resetSection: (section)=>set((state)=>{
                switch(section){
                    case "formData":
                        state.formData = {};
                        state.formErrors = {};
                        state.formTouched = {};
                        break;
                    case "loadingStates":
                        state.loadingStates = {};
                        break;
                    case "errors":
                        state.errors = {};
                        break;
                    case "toasts":
                        state.toasts = [];
                        break;
                    case "filters":
                        state.filters = {};
                        break;
                    case "pagination":
                        state.pagination = {};
                        break;
                    case "selections":
                        state.selections = {};
                        break;
                    default:
                        _logger.logger.warn("Unknown section to reset", {
                            section
                        });
                }
            })
    })), {
    name: "ui-store",
    partialize: (state)=>({
            activeTab: state.activeTab,
            sidebarOpen: state.sidebarOpen,
            viewMode: state.viewMode,
            sortBy: state.sortBy,
            sortOrder: state.sortOrder
        })
}), {
    name: "ui-store"
}));
const useActiveTab = ()=>useUIStore((state)=>state.activeTab);
const useSetActiveTab = ()=>useUIStore((state)=>state.setActiveTab);
const useActiveModal = ()=>useUIStore((state)=>state.activeModal);
const useModalData = ()=>useUIStore((state)=>state.modalData);
const useFormData = (key)=>useUIStore((state)=>state.formData[key]);
const useFormError = (key)=>useUIStore((state)=>state.formErrors[key]);
const useFormTouched = (key)=>useUIStore((state)=>state.formTouched[key]);
const useLoading = (key)=>useUIStore((state)=>state.loadingStates[key] || false);
const useError = (key)=>useUIStore((state)=>state.errors[key]);
const useToasts = ()=>useUIStore((state)=>state.toasts);
const useSidebarOpen = ()=>useUIStore((state)=>state.sidebarOpen);
const useDrawerOpen = ()=>useUIStore((state)=>state.drawerOpen);
const useSearchQuery = ()=>useUIStore((state)=>state.searchQuery);
const useSearchResults = ()=>useUIStore((state)=>state.searchResults);
const useSearchLoading = ()=>useUIStore((state)=>state.searchLoading);
const useFilters = (key)=>useUIStore((state)=>state.filters[key]);
const usePagination = (key)=>useUIStore((state)=>state.pagination[key]);
const useSelections = (key)=>useUIStore((state)=>state.selections[key]);
const useViewMode = (key)=>useUIStore((state)=>state.viewMode[key]);
const useSortBy = (key)=>useUIStore((state)=>state.sortBy[key]);
const useSortOrder = (key)=>useUIStore((state)=>state.sortOrder[key]);
const useUIActions = ()=>useUIStore((state)=>({
            setActiveTab: state.setActiveTab,
            goBackTab: state.goBackTab,
            clearTabHistory: state.clearTabHistory,
            openModal: state.openModal,
            closeModal: state.closeModal,
            closeAllModals: state.closeAllModals,
            setFormData: state.setFormData,
