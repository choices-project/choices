                                    ]
                                })
                            })
                        ]
                    })
                });
            default:
                return /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                    className: "space-y-4",
                    children: /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                        className: "bg-white dark:bg-gray-800 rounded-lg shadow-sm border border-gray-200 dark:border-gray-700 p-4",
                        children: [
                            /*#__PURE__*/ (0, _jsxruntime.jsx)("h3", {
                                className: "text-lg font-semibold text-gray-900 dark:text-white mb-2",
                                children: "Welcome to Choices"
                            }),
                            /*#__PURE__*/ (0, _jsxruntime.jsx)("p", {
                                className: "text-sm text-gray-600 dark:text-gray-400",
                                children: "Your comprehensive civic engagement platform"
                            })
                        ]
                    })
                });
        }
    };
    return /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
        className: `min-h-screen bg-gray-50 dark:bg-gray-900 ${className}`,
        children: [
            /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                className: "md:hidden flex items-center justify-between p-4 bg-white dark:bg-gray-800 shadow-sm",
                children: [
                    /*#__PURE__*/ (0, _jsxruntime.jsx)("button", {
                        onClick: ()=>setSidebarOpen(true),
                        "data-testid": "hamburger-menu",
                        className: "p-2 rounded-md text-gray-700 dark:text-gray-300",
                        children: /*#__PURE__*/ (0, _jsxruntime.jsx)(_outline.Bars3Icon, {
                            className: "w-6 h-6"
                        })
                    }),
                    /*#__PURE__*/ (0, _jsxruntime.jsx)("h1", {
                        className: "text-xl font-bold text-gray-900 dark:text-white",
                        children: "Choices"
                    }),
                    /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                        className: "flex items-center space-x-2",
                        children: [
                            /*#__PURE__*/ (0, _jsxruntime.jsx)("button", {
                                onClick: toggleDarkMode,
                                "data-testid": "theme-toggle",
                                className: "p-2 rounded-md text-gray-700 dark:text-gray-300",
                                children: isDarkMode ? /*#__PURE__*/ (0, _jsxruntime.jsx)(_outline.SunIcon, {
                                    className: "w-5 h-5"
                                }) : /*#__PURE__*/ (0, _jsxruntime.jsx)(_outline.MoonIcon, {
                                    className: "w-5 h-5"
                                })
                            }),
                            /*#__PURE__*/ (0, _jsxruntime.jsx)("button", {
                                className: "p-2 rounded-md text-gray-700 dark:text-gray-300",
                                children: /*#__PURE__*/ (0, _jsxruntime.jsx)(_outline.BellIcon, {
                                    className: "w-6 h-6"
                                })
                            })
                        ]
                    })
                ]
            }),
            /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                className: "bg-blue-50 dark:bg-blue-900 border-b border-gray-200 dark:border-gray-700 p-2",
                children: /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                    className: "flex items-center justify-between text-sm",
                    children: [
                        /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                            className: "flex items-center space-x-4",
                            children: [
                                /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                    className: "flex items-center space-x-1",
                                    "data-testid": isOnline ? "online-indicator" : "offline-indicator",
                                    children: [
                                        isOnline ? /*#__PURE__*/ (0, _jsxruntime.jsx)(_outline.WifiIcon, {
                                            className: "w-4 h-4 text-green-500"
