                                lastSync.toLocaleTimeString()
                            ]
                        })
                    ]
                })
            }),
            /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                className: "bg-white dark:bg-gray-800 border-b border-gray-200 dark:border-gray-700",
                children: /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                    className: "flex",
                    children: [
                        {
                            id: "feed",
                            name: "Feed",
                            icon: _outline.HomeIcon
                        },
                        {
                            id: "representatives",
                            name: "Representatives",
                            icon: _outline.UserGroupIcon
                        },
                        {
                            id: "analytics",
                            name: "Analytics",
                            icon: _outline.ChartBarIcon
                        }
                    ].map((tab)=>/*#__PURE__*/ (0, _jsxruntime.jsxs)("button", {
                            onClick: ()=>setActiveTab(tab.id),
                            className: `flex-1 flex items-center justify-center space-x-2 py-3 px-4 text-sm font-medium transition-colors ${activeTab === tab.id ? "text-blue-600 border-b-2 border-blue-600 bg-blue-50 dark:bg-blue-900" : "text-gray-600 dark:text-gray-400 hover:text-gray-900 dark:hover:text-white hover:bg-gray-50 dark:hover:bg-gray-700"}`,
                            children: [
                                /*#__PURE__*/ (0, _jsxruntime.jsx)(tab.icon, {
                                    className: "w-5 h-5"
                                }),
                                /*#__PURE__*/ (0, _jsxruntime.jsx)("span", {
                                    children: tab.name
                                })
                            ]
                        }, tab.id))
                })
            }),
            /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                className: `fixed inset-0 bg-gray-800 bg-opacity-75 z-40 md:hidden ${sidebarOpen ? "block" : "hidden"}`,
                onClick: ()=>setSidebarOpen(false)
            }),
            /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                "data-testid": "mobile-nav",
                className: `fixed inset-y-0 left-0 w-64 bg-white dark:bg-gray-800 shadow-lg z-50 transform ${sidebarOpen ? "translate-x-0" : "-translate-x-full"} md:hidden transition-transform duration-300 ease-in-out`,
                children: [
                    /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                        className: "p-4 border-b border-gray-200 dark:border-gray-700 flex items-center justify-between",
