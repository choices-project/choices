const testStorePersistence = async (store, key, expectedValue)=>{
    // Set value
    store.setState(expectedValue);
    // Simulate persistence
    const persisted = JSON.parse(localStorage.getItem(key) || "{}");
    expect(persisted).toMatchObject(expectedValue);
};
const testStoreMiddleware = (store, middleware, action, expectedBehavior)=>{
    // Test middleware behavior
    action(store.getState());
    expectedBehavior();
};
const testStorePerformance = (store, action, maxDuration = 10)=>{
    const startTime = performance.now();
    action(store.getState());
    const endTime = performance.now();
    const duration = endTime - startTime;
    expect(duration).toBeLessThan(maxDuration);
};
const testStoreErrorHandling = (store, errorAction, expectedError)=>{
    expect(()=>{
        errorAction(store.getState());
    }).toThrow(expectedError);
};
const testStoreStateTransition = (store, initialState, actions, expectedFinalState)=>{
    // Set initial state
    store.setState(initialState);
    // Apply actions
    actions.forEach((action)=>{
        action(store.getState());
    });
    // Check final state
    const finalState = store.getState();
    expect(finalState).toMatchObject(expectedFinalState);
};
const testStoreMultipleSubscriptions = (store, selector, listeners)=>{
    const unsubscribes = listeners.map((listener)=>store.subscribe(selector, listener));
    // Test all listeners
    const testValue = selector(store.getState());
    listeners.forEach((listener)=>listener(testValue));
    // Cleanup
    unsubscribes.forEach((unsubscribe)=>unsubscribe());
};
