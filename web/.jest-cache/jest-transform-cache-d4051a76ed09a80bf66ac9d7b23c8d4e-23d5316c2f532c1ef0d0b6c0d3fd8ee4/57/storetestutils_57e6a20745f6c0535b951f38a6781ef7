const testStoreSideEffects = (store, action, sideEffect)=>{
    const mockSideEffect = jest.fn(sideEffect);
    action(store.getState());
    expect(mockSideEffect).toHaveBeenCalled();
};
const testStoreWithDependencies = (store, dependencies, action)=>{
    action(store.getState(), dependencies);
    // Test that dependencies were used correctly
    Object.values(dependencies).forEach((dep)=>{
        if (typeof dep === "function") {
            expect(dep).toHaveBeenCalled();
        }
    });
};
const testStoreValidation = (store, validator, invalidAction)=>{
    const initialState = store.getState();
    expect(validator(initialState)).toBe(true);
    expect(()=>{
        invalidAction(store.getState());
    }).toThrow();
};
const testStoreTimeBased = (store, timeAction, expectedDuration)=>{
    const startTime = Date.now();
    timeAction(store.getState());
    const endTime = Date.now();
    const duration = endTime - startTime;
    expect(duration).toBeGreaterThanOrEqual(expectedDuration);
};
const testStoreCleanup = (store, action, cleanup)=>{
    action(store.getState());
    cleanup();
    // Verify cleanup was called
    expect(cleanup).toHaveBeenCalled();
};
const testStoreConcurrent = (store, actions, expectedFinalState)=>{
    // Run actions concurrently
    const promises = actions.map((action)=>Promise.resolve(action(store.getState())));
    return Promise.all(promises).then(()=>{
        const finalState = store.getState();
        expect(finalState).toMatchObject(expectedFinalState);
    });
};
const testStoreErrorRecovery = (store, errorAction, recoveryAction, expectedRecoveredState)=>{
    // Trigger error
    expect(()=>{
        errorAction(store.getState());
    }).toThrow();
    // Test recovery
    recoveryAction(store.getState());
    const recoveredState = store.getState();
    expect(recoveredState).toMatchObject(expectedRecoveredState);
};
const testStoreSnapshots = (store, actions, expectedSnapshots)=>{
    const snapshots = [];
    // Capture initial state
    snapshots.push(store.getState());
    // Apply actions and capture snapshots
    actions.forEach((action)=>{
        action(store.getState());
        snapshots.push(store.getState());
    });
    // Compare snapshots
    expect(snapshots).toEqual(expectedSnapshots);
};
