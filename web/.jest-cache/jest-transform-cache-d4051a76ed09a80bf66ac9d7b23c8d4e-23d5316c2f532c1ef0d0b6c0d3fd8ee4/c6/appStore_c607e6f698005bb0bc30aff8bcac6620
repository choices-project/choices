const _middleware = require("zustand/middleware");
const _immer = require("zustand/middleware/immer");
const _objects = require("../utils/objects");
// Default settings
const defaultSettings = {
    animations: true,
    haptics: true,
    sound: true,
    autoSave: true,
    language: "en",
    timezone: "UTC",
    compactMode: false,
    showTooltips: true,
